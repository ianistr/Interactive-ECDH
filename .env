# API Keys for client authentication
# Generate secure random keys - NEVER commit these to git! Add to .env!
API_KEY_ALICE=your_secure_random_key_here_change_me
API_KEY_BOB=another_secure_random_key_here_change_me

# SSL/TLS Configuration
SSL_KEYFILE=key.pem
SSL_CERTFILE=cert.pem

# Optional: Server Configuration Limits
MAX_MESSAGE_SIZE=102400
MAX_CONNECTIONS=1000
WS_AUTH_TIMEOUT=5

# Optional: CORS Configuration (comma-separated origins)
ALLOWED_ORIGINS=*